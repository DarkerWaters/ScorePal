        android:background="@color/primaryDarkColor"
        android:gravity="center"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintStart_toStartOf="@+id/teamOne_setFour"
        app:layout_constraintEnd_toEndOf="@+id/teamOne_setFour"
        app:layout_constraintTop_toBottomOf="@+id/tieBreak_setFour" />

    <uk.co.darkerwaters.scorepal.ui.views.ResizeTextView
        android:id="@+id/teamTwo_setFive"
        android:layout_width="0dp"
