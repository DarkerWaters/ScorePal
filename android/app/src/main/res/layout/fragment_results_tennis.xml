        android:textColor="@color/secondaryLightColor"
        app:layout_constraintBottom_toBottomOf="@+id/teamOne_setOne"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/teamOne_setOne" />

