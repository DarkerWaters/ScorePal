    <TextView
        android:id="@+id/textViewPointsTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/points"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/primaryDarkColor"
        app:layout_constraintEnd_toEndOf="@+id/teamOne_Points"
        app:layout_constraintStart_toStartOf="@+id/teamOne_Points"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewSet1Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/one"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/primaryDarkColor"
        app:layout_constraintEnd_toEndOf="@+id/teamOne_setOne"
        app:layout_constraintStart_toStartOf="@+id/teamOne_setOne"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewSet2Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/two"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/primaryDarkColor"
        app:layout_constraintEnd_toEndOf="@+id/teamOne_setTwo"
        app:layout_constraintStart_toStartOf="@+id/teamOne_setTwo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewSet3Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
